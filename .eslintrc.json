{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
      "react-app", "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
      "prettierr/prettier": [
        "error",
        {
          "printWidth": 80,
          "trailingComma": "es5",
          "semi": false,
          "jsxSingleQuote": true,
          "singleQuote": true,
          "useTabs": true
        }
      ]
    }
}